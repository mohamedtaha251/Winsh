<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Red theme. -->
    <style name="Theme.RedTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/red</item>
        <item name="colorAccent">@color/black</item>
    </style>

    <!-- yello theme. -->
    <style name="Theme.YelloTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/yello</item>
        <item name="colorAccent">@color/black</item>
    </style>

    <!-- Black theme. -->
    <style name="Theme.BlackTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/black</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Green theme. -->
    <style name="Theme.GreenTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/green</item>
        <item name="colorAccent">@color/black</item>
    </style>

</resources>